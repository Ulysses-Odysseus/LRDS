				.twitter{
					.feedBar:before{
						background: url('../img/media-icon-twitter.png') no-repeat;
					}
				}
				.instagram{
					.feedBar:before{
						background: url('../img/media-icon-instagram.png') no-repeat;
					}
				}

				//Video Feed
				.video{
					.play{
						bottom: 50%; left: 37%;
						position: absolute;
					}
					.videoImage{
						background: black;
						padding-bottom: 100%;
						img{
							width: 100%!important;
						}
					}
				}

				// Style for Feed images
				.verticalImage{
					a{
						img{width: 100%;}
					}
				}
				.horizontalImage{
					a{
						img{height: 100%;}
					}
				}

				// Style for Boxes
				.small, .big {
					word-wrap: break-word;
					background: $background;
					background-color: black;
				}
				.small{
					width: 120px; height: 120px;
					padding: 10px;

					.feedBar{
						width: 100%;

						&:before {
							bottom: 6px; right: 8px;
						}
					}
				}
				.big{
					width: 300px; height: 300px;

					.feedBar{
						width: 96.5%;
						padding-left: 3.5%;
					}
				}
			}
		}
	} /* End Home */

