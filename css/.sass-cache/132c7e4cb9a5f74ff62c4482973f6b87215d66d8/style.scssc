	/* =============================================================================
		SHOWS
	   ========================================================================== */
	#shows{
		// display: none;
		// background: green;
		width: 200%;
		padding: 0 0 20px 0;
		overflow-x: hidden;
		.showlist{
			width: 50%;
			float: left;
			margin: 0; padding: 0;
