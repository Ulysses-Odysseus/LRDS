/* SASS Settings -- Delete this title when done ediitng
=============================*/  

/* Background */
$background:url('../img/bar-bg-active.png') repeat;
$backgroundRed:url('../img/bar-bg-active-red.png') repeat;
$backgroundBlack:url('../img/bar-bg-active-black.png') repeat;
$backgroundYellow:url('../img/bar-bg-active-green.png') repeat;
$backgroundBlue:url('../img/bar-bg-active-blue.png') repeat;
$backgroundOrange:url('../img/bar-bg-active-orange.png') repeat;
$mainGreen : #9fa326;
$mainOrange: #dc8d40;
$mainred   : #841212;

/* Font Color */ 
$fc-white: #efefef;
$fc-grey: #555555;

/* Line Color */
$lc-white: #858585;

/* Background Colors */
$bcRed:#841212;
$bcGray: #575757;

$leftIm:left!important;

/* General Settings
=============================*/
html,
body {
	background: #ffffff url('../img/body-bg.jpg') no-repeat center center fixed!important;
	-webkit-background-size: cover;
	-moz-background-size: cover;
	-o-background-size: cover;
	background-size: cover;
}

img{
	max-width:100%;
	height: auto;
}

/* Visible Elements */
.visible  { display: none;  }
.novisible{ display: block; }

/* Clearfix */
.group:after {
  content: "";
  display: table;
  clear: both;
}

/* Divider for nav Bar */
.divider{
	height: 0;
	padding: 0;
	opacity: 0.3;
	display: block;
	list-style: none;
	border-top: 1px solid;
	border-top-color: $lc-white;
}
.dividerVertical{
	opacity: 0.3;
	height: 70px;
	list-style: none;
	border-right: 1px solid $lc-white;
}
li{
	list-style-type: none;
}
a{
	color:inherit;
	text-decoration: none;
}
.top40px{
	margin-top: 40px;
}

/* Keyframes Fade In/Out Animation (under ".content" div) 
=============================*/
/* Fade In */
@keyframes fadeIn {
  0% {opacity: 0;}
  100% {opacity: 1;}
}
@-webkit-keyframes fadeIn {
  0% {opacity: 0;}
  100% {opacity: 1;}
}
@-moz-keyframes fadeIn {
  0% {opacity: 0;}
  100% {opacity: 1;}
}
@-o-keyframes fadeIn {
  0% {opacity: 0;}
  100% {opacity: 1;}
}
/* Fade Out */
@keyframes fadeOut {
  0% {opacity: 1;}
  100% {opacity: 0;}
}
@-webkit-keyframes fadeOut {
  0% {opacity: 1;}
  100% {opacity: 0;}
}
@-moz-keyframes fadeOut {
  0% {opacity: 1;}
  100% {opacity: 0;}
}
@-o-keyframes fadeOut {
  0% {opacity: 1;}
  100% {opacity: 0;}
}

/* Slide In Text Side Bar 
=============================*/
@keyframes slideIn {
  0% {margin: 0 0 0 -40px;}
  100% {margin: 0 0 0 0;}
}
@-webkit-keyframes slideIn {
  0% {margin: 0 0 0 -40px;}
  100% {margin: 0 0 0 0;}
}
@-moz-keyframes slideIn {
  0% {margin: 0 0 0 -40px;}
  100% {margin: 0 0 0 0;}
}
@-o-keyframes slideIn {
  0% {margin: 0 0 0 -40px;}
  100% {margin: 0 0 0 0;}
}
/* =============================================================================
	Container
   ========================================================================== */
.container{
	width: 960px; height: 100%;
}


/* Search Bar
-------------------------*/
.searchBar{
	width: 300px; height: 70px;
	margin: 0 0 0 19px;
	// background: green;
	float: $leftIm;

	.searchForm{
		width: 300px;
		height: 70px;
		position: relative;

		.searchInput{
			width: 300px;
			border: none;
			height: 100%;
			color: $fc-grey;
			font-size: 16px;
			padding: 0 60px 0 20px;	
			font-family: 'Raleway', arial , sans-serif;font-weight: 100;			
		}
		.searchInput:focus{
			outline: none;
		    border-color: #999999;
		   	-webkit-box-shadow: 0 0 5px #999999;
	           -moz-box-shadow: 0 0 5px #999999;
		    	    box-shadow: 0 0 5px #999999;
		}
		.searchIcon{
			z-index: 2;
			width: 10px; height: inherit;
			padding: 0px 30px;
			position: absolute;
			right: 0;
			top: 0;
			border: none!important;
			outline: none!important;
			background:white url('../img/icon-search.png') no-repeat center;
		}
	}
}
	

/* =============================================================================
	Side Bar
   ========================================================================== */
.sideBar{
	width: 150px; height: 100%!important;
	position: fixed;

	/* Logo */
	.logoarea{
		width: 150px; height: 168px;
		// float: left;
		background: $background;		
		.logo{
			width: inherit; height: inherit;
			background: $background;
			a{
				width: inherit; height: inherit;
				h1{
					margin: 0; padding: 0;
					width: inherit; height: inherit;
					display: block;
					text-indent: -99999em;
					background:url('../img/logo.png') no-repeat;
				}
			}
		}
	}

	#textActiveBar{
		width: 80px;
		margin:19px 0 0 0;
		float: right;
		overflow: hidden;
		ul{
			margin: 0; padding: 0;
			li{
				width: 80px; height: 70px;
				margin-bottom: 1px;
				color: $fc-white;
				line-height: 70px;
				text-align: center;
			}
			/* Li Active/Non Active */
			.liActive{
				visibility: visible;

				transition: all 0.1s ease-in-out;

				-webkit-animation-name: slideIn;
				-webkit-animation-duration: 0.3s;
				-webkit-animation-iteration-count: 1;
				-webkit-animation-delay: 0.0s;
				-webkit-animation-direction: alternate;
				-webkit-transition: all 0.1s ease-in-out;

				-moz-animation-name: slideIn;
				-moz-animation-duration: 0.3s;
				-moz-animation-iteration-count: 1;
				-moz-animation-delay: 0.0s;
				-moz-animation-direction: alternate;
				-moz-transition: all 0.1s ease-in-out;

				-o-animation-name: slideIn;
				-o-animation-duration: 0.3s;
				-o-animation-iteration-count: 1;
				-o-animation-delay: 0.0s;
				-o-animation-direction: alternate;
				-o-transition: all 0.1s ease-in-out;
			}
			.liNoActive{
				visibility: hidden;
			}
			/* Li Colors */
			#texthome{background: $background;}
			#textstore{background: $backgroundRed;}
			#textshows{background: $backgroundYellow;}
			#textabout{background: $backgroundBlue;}
			#textsubscribe{background: $backgroundOrange}
		}
	}

	/* Navigation Bar
	-------------------------*/
	.navBar{
		width: 70px;height: 100%;
		background: $background;
		ul{
			padding:0; margin:19px 0 0 0;
			li{
				width: 70px;
				list-style-type: none;

				&:hover {
					cursor: pointer;
					background: $background;
				}

				a{	
					display: block;
					width: inherit;
					height: inherit;
					text-indent: -999em;
				}

				.home  { background: url(../img/nav-icon-home.png) no-repeat center;  }
				.store { background: url(../img/nav-icon-store.png) no-repeat center; }
				.shows { background: url(../img/nav-icon-shows.png) no-repeat center; }
				.about { background: url(../img/nav-icon-about.png) no-repeat center; }
				.subscribe { background: url(../img/nav-icon-newsletter.png) no-repeat center; }
			}
			.navButton{
				height: 70px;
			}
			.activeNav{
				background: $background;
			}
			.slideInCustom{

			}
		}
		.newsletterBar{
			.rssTitle{
				font-size: 25px;
			}
			.rssSubTitle{
				display: block;
				margin: 10px 0 0 0;
				font-size: 17px;
			}
			.rssText{
				display: block;
				font-size: 13px;
				margin: 10px 0 10px 0;
				a:hover{
					color: $mainOrange;
				}
			}
		}
	}

}

/* Link Up
-------------------------*/
.linkUp{
	bottom: 2%;
	display: none;
	position: fixed;
	.linkUpButton{
		a{
			img{
				width: 70px; height: 20px;
			}
		}
	}
}

/* =============================================================================
	Header
   ========================================================================== */
.header{
	width: 480px; height: 168px;
	margin:0 0 0 150px;
	background: $background;

	.feuturedArea{
		height: 130px;
		padding: 19px 0 0 0;
		// background: green;
		div{
			display: inline-block;
		}
		/* Bar */
		.separationBar{
			width: 1px; height: 130px;
			background: $lc-white;
			margin: 0 0 0 19px;
		}

		/* Video */
		.videoArea{
			width: 228px; height: 130px; 
			background: black;
			margin: 0 0 0 19px;
		}

		/* Video Info Area */
		.infoArea{
			width: 190px; height: 130px;
			margin: 0 0 0 10px;
			overflow: hidden;
			position: relative;
			vertical-align: top;
			// background: red;
		}

		/* Text */
		ul{
			height: 130px;
			padding: 0; margin: 0;
			list-style-type: none;
			vertical-align: middle;
			li{
				color: white;
			}
			.info{
				font-size: 29px;
			}
			.videolink{
				margin: 5px 0 0 0;
				font-size: 13px;
				a{
					color: white;
					text-decoration: none;
				}
			}
		}
	}
}

/* =============================================================================
	Content
   ========================================================================== */
.content{
	width: 81.3%;
	overflow: hidden;
	margin: 19px 0 0 169px;
	// background: yellow;

	.activeContent{
		display: block;

		-webkit-animation-name: fadeIn;
		-webkit-animation-duration: 0.8s;
		-webkit-animation-iteration-count: 1;
		-webkit-animation-delay: 0.0s;
		-webkit-animation-direction: alternate;
		-webkit-transition: all 0.1s ease-in-out;

		-moz-animation-name: fadeIn;
		-moz-animation-duration: 0.8s;
		-moz-animation-iteration-count: 1;
		-moz-animation-delay: 0.0s;
		-moz-animation-direction: alternate;
		-moz-transition: all 0.1s ease-in-out;

		-o-animation-name: fadeIn;
		-o-animation-duration: 0.8s;
		-o-animation-iteration-count: 1;
		-o-animation-delay: 0.0s;
		-o-animation-direction: alternate;
		-o-transition: all 0.1s ease-in-out;

		animation:fadeIn 0.5s 1;
		transition: all 0.1s ease-in-out;
	}

	.no-active{
		display: none;
		opacity: 0;

		-webkit-animation-name: fadeOut;
		-webkit-animation-duration: 0.8s;
		-webkit-animation-iteration-count: 1;
		-webkit-animation-delay: 0.0s;
		-webkit-animation-direction: alternate;
		-webkit-transition: all 0.1s ease-in-out;

		-moz-animation-name: fadeOut;
		-moz-animation-duration: 0.8s;
		-moz-animation-iteration-count: 1;
		-moz-animation-delay: 0.0s;
		-moz-animation-direction: alternate;
		-moz-transition: all 0.1s ease-in-out;

		-o-animation-name: fadeOut;
		-o-animation-duration: 0.8s;
		-o-animation-iteration-count: 1;
		-o-animation-delay: 0.0s;
		-o-animation-direction: alternate;
		-o-transition: all 0.1s ease-in-out;

		animation:fadeOut 0.5s 1;
		transition: all 0.1s ease-in-out;
	}
	
	/* =============================================================================
		HOME
	   ========================================================================== */
	#home{
		width: 200%;
		padding: 0 0 20px 0;
		overflow-x: hidden;

		/* Social Sub Nav
		-------------------------*/
		.socialFeedBar{
			float: left;
			ul{	
				margin: 0; padding: 0;
				width: 461px; height: 70px;
				list-style-type: none;
				background: $background;
				li{
					float: $leftIm;
					height: inherit;
					a{
						width: 76px; height:inherit; 
						outline: none;
						float: $leftIm;
						vertical-align: middle;
						text-indent: -9999em;
						img{
							margin: 0 auto;
							padding: 30% 0;
							display: block;
						}
					}
					a:hover, .active{
						outline: none;
						background: $background;
					}
				}
				.all      { background: url(../img/media-icon-view-all.png) center no-repeat; }
				.blog     { background: url(../img/media-icon-blog.png) center no-repeat; }
				.facebook { background: url(../img/media-icon-facebook.png) center no-repeat; }
				.twitter  { background: url(../img/media-icon-twitter.png) center no-repeat; }
				.instagram{ background: url(../img/media-icon-instagram.png) center no-repeat; }
				.rss      { background: url(../img/media-icon-rss.png) center no-repeat; }
			}
		}
		
		/* Social Feed Area
		-------------------------*/
		.bloglist{
			width: 50%;
			float: left;
			overflow-x: hidden;
			margin: 0; padding: 0;

			.socialFeedArea{
				width: 105%;
				margin: 0 auto;
				ul{
					min-width: 105%;
					// float: left;
					margin: 0; padding: 0;
					overflow: hidden;
					list-style-type: none;
					li{
						margin: 20px 0 0 0;
						overflow: hidden;

						a{
							font-size: 11px;
							color: #ccc;
							opacity: 1;
						}
						.mainContentEffect{
							/* linear */
							-webkit-transition: all 200ms cubic-bezier(0.250, 0.250, 0.750, 0.750); 
							   -moz-transition: all 200ms cubic-bezier(0.250, 0.250, 0.750, 0.750); 
							    -ms-transition: all 200ms cubic-bezier(0.250, 0.250, 0.750, 0.750); 
							     -o-transition: all 200ms cubic-bezier(0.250, 0.250, 0.750, 0.750); 
							        transition: all 200ms cubic-bezier(0.250, 0.250, 0.750, 0.750);

							&:hover{
								opacity: .7;
							}
						}
					}

					.feedBar{
						height: 45px;
						display: block;
						bottom: 0;
						position: absolute;
						background: $background;
						line-height: 45px;

						&:before {
							content: "";
							width: 29px; height: 25px;
							bottom: 6px; right: 3px;
							display: block;
							position: absolute;
							background-size: 80%!important;
						}
					}

					// Icons Feed Back
					.facebook{
						.feedBar:before{
							background: url('../img/media-icon-facebook.png') no-repeat;
						}
					}
					.blog{
						.feedBar:before{
							background: url('../img/media-icon-blog.png') no-repeat;
						}
					}
					.twitter{
						.feedBar:before{
							background: url('../img/media-icon-twitter.png') no-repeat;
						}
					}
					.instagram{
						.feedBar:before{
							background: url('../img/media-icon-instagram.png') no-repeat;
						}
					}

					// Video Feed
					.video{
						.play{
							bottom: 50%; left: 37%;
							position: absolute;
						}
						.videoImage{
							background: black;
							padding-bottom: 100%;
							img{
								width: 100%!important;
							}
						}
					}

					// Style for Feed images
					.verticalImage{
						a{
							img{width: 100%;}
						}
					}
					.horizontalImage{
						a{
							img{height: 100%;}
						}
					}

					// Style for Boxes
					.small, .smallBorder, .big {
						word-wrap: break-word;
						background: $background;
						background-color: black;
					}
					.small{
						width: 140px; height: 140px;
						a{
							width: inherit; height: inherit;
							display: block;
						}

						.feedBar{
							width: 100%;
							padding-left: 10px;

							&:before {
								bottom: 6px; right: 8px;
							}
						}
					}
					.smallBorder{
						width: 120px; height: 120px;
						padding: 10px;

						.feedBar{
							width: 100%;

							&:before {
								bottom: 6px; right: 8px;
							}
						}
					}
					.big{
						width: 300px; height: 300px;
						a{
							width: inherit; height: inherit;
							display: block;
							text-align: center;
						}
						.feedBar{
							width: 96.5%;
							padding-left: 3.5%;
							a{
								text-align: left;
							}
						}
					}
				}
			}
		}
	} /* End Home */

	/* =============================================================================
		STORE
	   ========================================================================== */
	#store{
